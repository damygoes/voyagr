{
  "name": "@voyagr/ui",
  "version": "0.0.0",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts"
    },
    "./components/*": {
      "import": "./src/components/*"
    },
    "./styles.css": "./src/css/styles.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tailwindcss -i ./src/css/styles.css -o ./dist/index.css",
    "check-types": "tsc --noEmit",
    "dev": "tailwindcss -i ./src/css/styles.css -o ./dist/index.css --watch",
    "lint": "eslint src --max-warnings 0",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^19"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@svgr/cli": "^8.1.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@voyagr/eslint-config": "workspace:*",
    "@voyagr/tailwind-config": "workspace:*",
    "@voyagr/typescript-config": "workspace:*",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.25.0",
    "eslint-plugin-storybook": "^0.12.0",
    "postcss": "^8.5.3",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.8.2",
    "vite": "^6.3.5",
    "vite-plugin-svgr": "^4.3.0"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@voyagr/utils": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "lucide-react": "^0.511.0",
    "react-hook-form": "^7.56.4",
    "zod": "^3.25.32"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
